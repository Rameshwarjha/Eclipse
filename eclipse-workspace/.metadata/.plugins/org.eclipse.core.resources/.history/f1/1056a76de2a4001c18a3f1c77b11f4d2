package filehandler;

import java.io.*;

public class ReadDataIStream {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		try {
			FileInputStream fi =new FileInputStream("E://new//createFileUsingCode.txt");
			int i = 0;
			
			while ((i=fi.read())!=-1) {
				System.out.println((char)i);
				
			}
			fi.close();
		}
		catch(Exception e ) {
			e.printStackTrace();
		}
		
		
		

	}

}
